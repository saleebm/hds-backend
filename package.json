    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "body-parser": "^1.19.0",
    "commitizen": "^4.1.2",
    "confusing-browser-globals": "^1.0.9",
    "core-js": "^3.6.5",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-prettier-standard": "^3.0.1",
